<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.2.1/css/all.min.css">
    
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css">
<style>
   body{
    background-color:white;
   }
</style>
</head>

<body>
    <div class="container">
    <table class="table table-darks table-striped">
        <thead>
            <tr>
                <th>SI</th>
                <th>TRAIN NO</th>
                <th>TRAIN NAME</th>
                <th>SOURCE</th>
                <th>DESTINATION</th>
            </tr>
        </thead>
        <tbody>
            <% user1.forEach(function(user1){%>
                <tr>
                    <td>
                        <%=user1.id%>
                    </td>
                    <td>
                        <%=user1.train_no%>
                    </td>
                    <td>
                        <%=user1.train_name%>
                    </td>
                    <td>
                        <%=user1.source%>
                    </td>
                    <td>
                        <%=user1.destination%>
                    </td>
                    <td>
                       <a href="book/<%=user1.id%>" class="btn btn-sm btn-success">BOOK</a>
                       
                    </td>
                </tr>

                <%}); %>
        </tbody>
    </table>
</div>
</body>

</html>